.wxParse {
    margin: 0 10rpx;
    font-family: Helvetica,sans-serif;
    font-size: 28rpx;
    color: #666;
    line-height: 1.8;
}

view {
    word-break: break-all;
}

.wxParse-inline {
    display: inline;
    margin: 0;
}

.wxParse-div {
    margin: 0;
    padding: 0;
}

.wxParse-h1 {
    font-size: 2em;
    margin: 0.67em 0;
}

.wxParse-h2 {
    font-size: 1.5em;
    margin: 0.75em 0;
}

.wxParse-h3 {
    font-size: 1.17em;
    margin: 0.83em 0;
}

.wxParse-h4 {
    margin: 1.12em 0;
}

.wxParse-h5 {
    font-size: .83em;
    margin: 1.5em 0;
}

.wxParse-h6 {
    font-size: .75em;
    margin: 1.67em 0;
}

.wxParse-h1 {
    font-size: 36rpx;
    font-weight: 400;
    margin-bottom: .9em;
}

.wxParse-h2 {
    font-size: 32rpx;
    font-weight: 400;
    margin-bottom: .34em;
}

.wxParse-h3 {
    font-weight: 400;
    font-size: 30rpx;
    margin-bottom: .34em;
}

.wxParse-h4 {
    font-weight: 400;
    font-size: 28rpx;
    margin-bottom: .24em;
}

.wxParse-h5 {
    font-weight: 400;
    font-size: 26rpx;
    margin-bottom: .14em;
}

.wxParse-h6 {
    font-weight: 400;
    font-size: 24rpx;
    margin-bottom: .04em;
}

.wxParse-h1,.wxParse-h2,.wxParse-h3,.wxParse-h4,.wxParse-h5,.wxParse-h6,.wxParse-b,.wxParse-strong {
    font-weight: bolder;
}

.wxParse-i,.wxParse-cite,.wxParse-em,.wxParse-var,.wxParse-address {
    font-style: italic;
    line-height: 0.8;
}

.wxParse-pre,.wxParse-tt,.wxParse-code,.wxParse-kbd,.wxParse-samp {
    font-family: monospace;
}

.wxParse-pre {
    white-space: pre;
}

.wxParse-big {
    font-size: 1.17em;
}

.wxParse-small,.wxParse-sub,.wxParse-sup {
    font-size: 0.83em;
}

.wxParse-sub {
    vertical-align: sub;
}

.wxParse-sup {
    vertical-align: super;
}

.wxParse-s,.wxParse-strike,.wxParse-del {
    text-decoration: line-through;
}

.wxParse-strong,.wxParse-s {
    display: inline;
}

.wxParse-a {
    color: deepskyblue;
    word-break: break-all;
    overflow: auto;
}

.wxParse-video {
    text-align: center;
    margin: 20rpx 0;
}

.wxParse-video-video {
    width: 100%;
}

.wxParse-img {
    overflow: hidden;
    width: 100%;
    display: block;
}

.wxParse-blockquote {
    margin: 0;
    padding: 20rpx 0 20rpx 10rpx;
    font-family: Courier,Calibri,"宋体";
    background: #f5f5f5;
    border-left: 6rpx solid #dbdbdb;
}

.wxParse-code,.wxParse-wxxxcode-style {
    display: inline;
    background: #f5f5f5;
}

.wxParse-ul {
    margin: 20rpx 10rpx;
}

.wxParse-li,.wxParse-li-inner {
    display: flex;
    align-items: baseline;
    margin: 10rpx 0;
}

.wxParse-li-text {
    align-items: center;
    line-height: 40rpx;
}

.wxParse-li-circle {
    display: inline-flex;
    width: 10rpx;
    height: 10rpx;
    background-color: #333;
    margin-right: 10rpx;
}

.wxParse-li-square {
    display: inline-flex;
    width: 10rpx;
    height: 10rpx;
    background-color: #333;
    margin-right: 10rpx;
}

.wxParse-li-ring {
    display: inline-flex;
    width: 10rpx;
    height: 10rpx;
    border: 2rpx solid #333;
    border-radius: 50%;
    background-color: #fff;
    margin-right: 10rpx;
}

.wxParse-u {
    text-decoration: underline;
}

.wxParse-hide {
    display: none;
}

.WxEmojiView {
    align-items: center;
}

.wxEmoji {
    width: 32rpx;
    height: 32rpx;
}

.wxParse-tr {
    display: flex;
    border-right: 2rpx solid #e0e0e0;
    border-bottom: 2rpx solid #e0e0e0;
    border-top: 2rpx solid #e0e0e0;
}

.wxParse-th,.wxParse-td {
    flex: 1;
    padding: 10rpx;
    font-size: 28rpx;
    border-left: 2rpx solid #e0e0e0;
    word-break: break-all;
}

.wxParse-td:last {
    border-top: 2rpx solid #e0e0e0;
}

.wxParse-th {
    background: #f0f0f0;
    border-top: 2rpx solid #e0e0e0;
}

.wxParse-del {
    display: inline;
}

.wxParse-figure {
    overflow: hidden;
}

.product-detail-wrapper {
    overflow: auto;
}

.product-detail-wrapper .product-content {
    position: relative;
}

.product-detail-wrapper .product-content .richText {
    padding: 10rpx 0 30rpx;
}

.form button {
    background: #ffffff;
    border: 0;
    padding: 0;
    border-radius: 0;
}

.content-wrapper {
    background: #f4f4f4;
    transition: opacity .4s;
    overflow: auto;
}

.content-wrapper.show-variation {
    opacity: 0.4;
}

.page-wrapper {
    background: #fff;
    border-top: 2rpx solid #f4f4f4;
    padding: 30rpx;
    margin-bottom: 20rpx;
}

.text-wrapper {
    position: relative;
}

.text-wrapper .title,.text-wrapper .price,.text-wrapper .description {
    display: block;
    margin-bottom: 14rpx;
}

.text-wrapper .description {
    color: #555;
}

.text-wrapper .title {
    font-size: 36rpx;
    font-weight: bold;
    max-width: 70%;
}

.text-wrapper .price {
    font-size: 34rpx;
    font-weight: bold;
    padding-left: 0;
    text-align: left;
}

.text-wrapper .center-item {
    display: flex;
    justify-content: space-between;
}

.text-wrapper .center-item .title {
    display: flex;
}

.text-wrapper .share {
    display: flex;
}

.text-wrapper .share-text {
    font-size: 24rpx;
    height: 30rpx;
    color: #5b5959;
    display: inline-block;
}

.text-wrapper .share-img {
    width: 31rpx;
    height: 30rpx;
    padding-left: 10rpx;
}

.text-wrapper .description {
    font-size: 32rpx;
    line-height: 1.4;
    margin-bottom: 0;
}

.text-wrapper .flash-price-wrapper {
    margin-bottom: 10rpx;
}

.text-wrapper .flash-price-wrapper .flash-price {
    font-size: 44rpx;
    font-weight: 400;
    padding-left: 0;
    text-align: left;
    color: #ec5360;
    margin-bottom: 10rpx;
}

.text-wrapper .flash-price-wrapper .flash-label {
    color: #fff;
    font-size: 26rpx;
    background: red;
    padding: 4rpx;
    border-radius: 6rpx;
    display: inline-block;
    margin-right: 10rpx;
}

.text-wrapper .flash-price-wrapper .old-price {
    color: #b8bdc1;
    text-decoration: line-through;
}

.main-button.out-of-stock {
    background: #cecece!important;
}

.quantity-error {
    color: #ec5360;
    padding-right: 20rpx;
}

.quantity {
    vertical-align: middle;
    padding: 0 20rpx;
}

.animate-image {
    height: 480rpx;
    position: absolute;
    bottom: calc(-140%);
    left: 0;
    background: #f0f0f0;
}

.product-detail {
    padding: 36rpx 0;
    background: #fff;
}

.product-detail text {
    word-break: break-word;
}

.slider-wrapper {
    position: relative;
    height: 562rpx;
    overflow: hidden;
}

.image-slider {
    height: 562rpx;
}

.slide-item image {
    width: 100%;
    height: 562rpx;
}

.slider-dots {
    position: absolute;
    bottom: 20rpx;
    text-align: center;
    width: 100vw;
}

.slider-dot {
    margin: 0 8rpx;
    display: inline-block;
    width: 12rpx;
    height: 12rpx;
    border-radius: 20rpx;
    border: 2rpx solid #ABABAB;
}

.slider-dot.selected {
    background: #fff;
    border: 2rpx solid #fff;
}

.price {
    color: #ec5360;
    vertical-align: middle;
    padding-left: 20rpx;
    text-align: right;
}

.pt100 {
    padding-bottom: 100rpx;
}

.dump {
    line-height: 0.8;
    margin: 0;
}

.dump view {
    color: transparent;
}

.dump .wxParse-inline {
    margin: 0;
}

.wxParse-p {
    padding: 0 40rpx;
}

.bottom {
    width: 100vw;
    position: fixed;
    bottom: 0;
}

.bottom .contact-icon {
    width: 100rpx;
    height: 100rpx;
    line-height: 90rpx;
    background: #fff;
    box-sizing: border-box;
    border-radius: 0;
}

.bottom .contact-icon button {
    padding: 0;
    border: 2rpx solid #ddd;
    border-right: none;
    background: #fff;
    width: 100rpx;
    height: 100rpx;
    line-height: 90rpx;
    color: #fff;
}

.ctrl-btns-fixed {
    margin-bottom: 150rpx;
}
.bottom button{
    border-radius: 0px;
}
.share-view111 image {
    width: 100%;
    height: 100%;
}
.share-view111 {
    padding-bottom: 100rpx;
    border-bottom: 1px solid #eee;
}